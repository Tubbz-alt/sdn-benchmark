{
	"outputFile":"auditRecords.gpb",
	"enabled":true,
	"queueConfig":{
		"highWaterMark":-1,
		"lowWaterMark":-1,
		"discardOldest":true,
		"recoveryStrategy":"DROP"
	},
	"log":{
		"defaultPolicy":"forward",
		"filter":[
			{
				"Event":[
					{
						"OFMessage":{
							"ECHO_REPLY":null,
							"FEATURES_REPLY":null,
							"GET_CONFIG_REQUEST":null,
							"STATS_REPLY":null,
							"STATS_REQUEST":null,
							"BARRIER_REQUEST":null,
							"PACKET_IN":null,
							"QUEUE_GET_CONFIG_REQUEST":null,
							"FEATURES_REQUEST":null,
							"QUEUE_GET_CONFIG_REPLY":null,
							"HELLO":null,
							"ECHO_REQUEST":null,
							"GET_CONFIG_REPLY":null,
							"BARRIER_REPLY":null
						},
						"detail.category":"OF"
					}
				]
			},
			{
				"Event":[
					{
						"OFMessage":{
							"PACKET_OUT":null,
							"FLOW_MOD":{
								"command":[
									"OFPFC_ADD",
									"OFPFC_DELETE_STRICT"
								]
							}
						},
						"detail.category":"OF",
						"detail.clientID":[
							"SELinkDiscoveryManager",
							"SELearningSwitch"
						]
					}
				],
				"Disposition":"info"
			},
			{
				"Event":[
					{
						"OFMessage":{
							"FLOW_REMOVED":{
								"reason":"OFPRR_IDLE_TIMEOUT"
							}
						},
						"detail.category":"OF"
					}
				],
				"Disposition":"info"
			},
			{
				"Event":[
					{
						"OFMessage":{
							"FLOW_MOD":{
								"command":"OFPFC_DELETE_STRICT"
							}
						},
						"detail.category":"OF",
						"detail.memberID":"<SEKernel>"
					}
				],
				"Disposition":"info"
			}
		]
	}
}